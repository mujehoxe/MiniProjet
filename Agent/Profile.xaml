<Page x:Class="Agent.ProfilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Agent"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Profile">

    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
            <Image Name="ProfilePicture" Margin="20" Height="100" Width="100" HorizontalAlignment="Left" Source="recive_serializabl_objects.png" Stretch="Fill" />
            <ListBox Margin="20" Background="{x:Null}"/>
        </StackPanel>
    </Grid>
</Page>
